{
    "name": "tekimax-ts",
    "version": "0.1.7",
    "description": "Type-safe Tekimax Client for Node.js, Browser, and Edge.",
    "homepage": "https://tekimax.com",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "scripts": {
        "generate": "kubb generate",
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "sdk",
        "openapi",
        "kubb"
    ],
    "author": "Tekimax <hello@tekimax.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/TEKIMAX/tekimax-ts.git"
    },
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@kubb/cli": "^4.15.0",
        "@kubb/core": "^4.15.0",
        "@kubb/plugin-client": "^4.20.0",
        "@kubb/plugin-oas": "^4.15.0",
        "@kubb/plugin-ts": "^4.20.0",
        "@kubb/plugin-zod": "^4.15.0",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.1",
        "typescript": "^5.3.3"
    }
}