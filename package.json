{
    "name": "tekimax-ts",
    "version": "0.1.0",
    "description": "Tekimax TypeScript SDK generated with Kubb",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.js",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "generate": "kubb generate",
        "build": "tsup src/gen/index.ts --format cjs,esm --dts",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "openresponses",
        "sdk",
        "openapi",
        "kubb"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@kubb/plugin-ts": "^4.20.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@kubb/cli": "^4.15.0",
        "@kubb/core": "^4.15.0",
        "@kubb/plugin-oas": "^4.15.0",
        "@kubb/plugin-zod": "^4.15.0",
        "ts-node": "^10.9.2",
        "tsup": "^8.5.1",
        "typescript": "^5.3.3"
    }
}