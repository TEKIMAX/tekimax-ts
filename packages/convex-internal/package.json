{
    "name": "@tekimax/convex",
    "version": "0.1.0",
    "private": true,
    "description": "Internal Convex AI agent & workflow features for the Tekimax platform",
    "main": "./dist/index.cjs",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            },
            "require": {
                "types": "./dist/index.d.cts",
                "default": "./dist/index.cjs"
            }
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts",
        "dev": "tsup src/index.ts --format cjs,esm --dts --watch"
    },
    "dependencies": {
        "tekimax-ts": "*"
    },
    "devDependencies": {
        "tsup": "^8.5.0",
        "typescript": "^5.7.0"
    }
}